{
    "name": "serafim/assets",
    "type": "library",
    "description": "Asset Compiler for Laravel 4",
    "keywords": [
        "css",
        "scss",
        "less",
        "javascript",
        "coffeescript",
        "assets",
        "minfy",
        "laravel",
        "compression",
        "minification"
    ],
    "homepage": "https://github.com/SerafimArts/Assets",
    "license":  "MIT",
    "require": {
        "php":                          ">=5.4.0"
    },
    "suggest": {
        "linkorb/jsmin-php":            "1.0.0",
        "natxet/CssMin":                "dev-master",
        "coffeescript/coffeescript":    "1.3.1",
        "leafo/scssphp":                "dev-master",
        "leafo/lessphp":                "dev-master"
    },
    "authors": [
        {
            "name":     "Nesmeyanov Kirill",
            "email":    "serafim@sources.ru",
            "homepage": "http://serafimarts.ru"
        }
    ],
    "autoload": {
        "psr-0": { "Assets": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
